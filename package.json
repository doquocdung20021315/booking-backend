{
  "name": "medical-booking",
  "version": "1.0.0",
  "description": "medical booking",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env BUILD_MODE=dev nodemon --inspect --exec ./node_modules/.bin/babel-node src/index.js",
    "test": "mocha --exit --s 0 --timeout 10000 src/test/api.test.js",
    "clean": "rd /s /q build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./build/src/index.js"
  },
  "keywords": [
    "medical",
    "booking"
  ],
  "author": "dung",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.4.0",
    "mongoose": "^8.0.1",
    "nodemailer": "^6.9.13",
    "supertest": "^6.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}